{
    "name": "anithemes/aniplay",
    "type": "wordpress-theme",
    "version": "1.0.0",
    "description": "Aniplay is undoubtedly the best and most powerful framework, with access to a large volume of content with a single click, completely optimized.",
    "keywords": [
        "WordPress",
        "Themes"
    ],
    "homepage": "https://github.com/Anithemes/aniplay",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Contributors",
            "homepage": "https://github.com/Anithemes/aniplay/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "wptrt/wpthemereview": "^0.2.1",
        "php-parallel-lint/php-parallel-lint": "^1.2.0",
        "wp-cli/i18n-command": "^2.2.5"
    },
    "scripts": {
        "lint:wpcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
        "lint:php": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor ."
    },
    "support": {
        "issues": "https://github.com/Anithemes/aniplay/issues",
        "source": "https://github.com/Anithemes/aniplay"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
